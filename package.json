{
  "name": "scli.js",
  "version": "1.0.0",
  "description": "An official package to init sdev projects. ",
  "main": "dist/index.js",
  "scripts": {
    "build:watch": "concurrently \"pnpm run format\" \"tsc --watch\"",
    "build": "concurrently \"pnpm run format\" \"tsc\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "concurrently \"pnpm run build\" \" node .\"",
    "start:dev": "concurrently \"pnpm run build\" \" nodemon .\""
  },
  "bin": {
    "sdev": "node ."
  },
  "keywords": [],
  "author": "Sdev",
  "license": "Apache-2.0 license",
  "dependencies": {
    "chalk": "4",
    "exec-sh": "^0.4.0",
    "figlet": "^1.5.2",
    "inquirer": "8",
    "nanospinner": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
